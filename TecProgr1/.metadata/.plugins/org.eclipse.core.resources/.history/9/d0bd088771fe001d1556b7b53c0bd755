package br.fatec.principal;

public abstract class Conta {
	private int nrConta;
	private double saldo;
	
	public Conta(int nrConta, Correntista[] correntistas, Data dataAbertura, double monto) {
		
	}
	
	public int getNrConta() {
		return nrConta;
	}
	public void setNrConta(int nrConta) {
		this.nrConta = nrConta;
	}
	public double getSaldo() {
		return saldo;
	}
	public void setSaldo(double saldo) {
		this.saldo = saldo;
	}
	
	public void fazerDeposito(double monto) {
		
	}
	public boolean fazerSaque(double monto) {
		
	}
	public double consultaSaldo() {
		
	}
	public boolean fazerTranferencia(Conta dest, double monto) {
		
	}
	
}
